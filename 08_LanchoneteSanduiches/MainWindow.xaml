<Window x:Class="_08_LanchoneteSanduiches.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_08_LanchoneteSanduiches"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="550">
    <Grid Margin="20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="212*"/>
            <ColumnDefinition Width="50*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="item1" HorizontalAlignment="Left" Margin="20,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Height="24" Width="390"/>
        <TextBlock x:Name="item2" HorizontalAlignment="Left" Margin="20,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Grid.Row="1" Height="24" Width="391"/>
        <TextBlock x:Name="item3" HorizontalAlignment="Left" Margin="20,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Grid.Row="2" Height="24" Width="391"/>
        <TextBlock x:Name="item4" HorizontalAlignment="Left" Margin="20,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Grid.Row="3" Height="24" Width="390"/>
        <TextBlock x:Name="item5" HorizontalAlignment="Left" Margin="20,22,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Grid.Row="4" Height="24" Width="392"/>
        <TextBlock x:Name="price1" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Height="24" Width="74" Grid.Column="1"/>
        <TextBlock x:Name="price2" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Center" FontSize="18" Height="24" Width="74" Grid.Column="1" Grid.Row="1"/>
        <TextBlock x:Name="price3" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Height="24" Width="74" Grid.Column="1" Grid.Row="2"/>
        <TextBlock x:Name="price4" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18" Height="24" Width="74" Grid.Column="1" Grid.Row="3"/>
        <TextBlock x:Name="price5" HorizontalAlignment="Left" Margin="10,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Center" FontSize="18" Height="24" Width="74" Grid.Column="1" Grid.Row="4"/>
    </Grid>
</Window>
